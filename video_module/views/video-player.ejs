<!DOCTYPE html>
<html>
  <head>
    <title>Video Player</title>
  </head>
  <body>
    <h1>Video Player</h1>
    <% if (files.length) { %>
    <ul>
      <% files.forEach(function (file) { %>
      <li>
        <a href="#" onclick="playVideo('/videos/<%= file %>')"><%= file %></a>

      </li>
      <% }) %>
    </ul>
    <% } else { %>
    <p>No videos found</p>
    <% } %>
    <div id="video-container"></div>
    <script>
      function playVideo(url) {
        //debugger;
  var container = document.getElementById('video-container');
  container.innerHTML = '<video controls playsinline><source src="' + url + '" type="video/mp4"></video>';
  
}
    </script>
  </body>
</html>
